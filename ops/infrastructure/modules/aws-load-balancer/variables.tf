variable "name" {
  type        = string
  description = "Name of instance"
}

variable "vpc" {
  type        = object({
    vpc_id     = string
    subnet_ids = list(string)
  })
  description = "VPC information"
}

variable "instance_ids" {
  type = list(string)
  description = "Instances to hide behind load balancer"
}
